{
  "project": {
    "version": "1.0.0-beta.20-4",
    "name": "ZeTalk"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.json",
      "prefix": "zp",
      "mobile": false,
      "styles": [
        "../node_modules/chartist/dist/chartist.css",
        "styles/index.scss"
      ],
      "scripts": [
        "../node_modules/webfontloader/webfontloader.js"
      ],
      "environments": {
        "source": "environments/environment.ts",
        "dev": "environments/environment.ts",
        "demo": "environments/environment.demo.ts",
        "local": "environments/environment.local.ts",
        "prod": "environments/environment.prod.ts",
        "aurelien": "environments/environment.aurelien.ts"
      }
    }
  ],
  "addons": [],
  "packages": [],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "prefixInterfaces": false,
    "inline": {
      "style": false,
      "template": false
    },
    "spec": {
      "class": false,
      "component": true,
      "directive": true,
      "module": false,
      "pipe": true,
      "service": true
    }
  }
}
